{
    "name": "dasundev/laravel-textit",
    "description": "Laravel package for Textit SMS Gateway in Sri Lanka",
    "type": "library",
    "keywords": [
        "textit",
        "sms",
        "gateway"
    ],
    "homepage": "https://github.com/dasundev/laravel-textit",
    "require": {
        "laravel/framework": "*",
        "guzzlehttp/guzzle": "^7.4",
        "php": "^7.2.5|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "autoload": {
        "files": [
            "src/helper.php"
        ],
        "psr-4": {
            "Dasundev\\LaravelTextit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dasundev\\LaravelTextit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dasundev\\LaravelTextit\\TextitServiceProvider"
            ],
            "aliases": {
                "Textit": "Dasundev\\LaravelTextit\\Support\\Facades\\Textit"
            }
        }
    },
    "authors": [
        {
            "name": "Dasun Tharanga",
            "email": "hello@dasun.dev"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
